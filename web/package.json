{
  "name": "web",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "start": "cpx ./serve.json ./out/ && serve out",
    "build": "yarn run swag && next build",
    "analyze": "cross-env ANALYZE=true next build",
    "format": "prettier --write './**/*.{ts,tsx}'",
    "lint": "eslint . && tsc --noEmit && jscpd . && knip && prettier --check './**/*.{ts,tsx}' && npmPkgJsonLint .",
    "lint:fix": "eslint . --fix",
    "swag": "npx openapi-typescript@5.4 ./docs/swagger.json -o ./src/api-wrapper/types/schema.d.ts --export-type",
    "test": "jest",
    "test:cov": "jest --coverage",
    "jest-preview": "jest-preview"
  },
  "dependencies": {
    "@mdx-js/loader": "3.1.0",
    "@mdx-js/react": "3.1.0",
    "@next/mdx": "15.3.2",
    "@popperjs/core": "2.11.8",
    "@sentry/nextjs": "9.17.0",
    "bootstrap": "5.3.6",
    "bootstrap-icons": "1.13.1",
    "cpx2": "8.0.0",
    "jest-preview": "0.3.1",
    "moment": "2.30.1",
    "moment-timezone": "0.5.48",
    "next": "15.3.2",
    "next-pwa": "5.6.0",
    "query-string": "9.1.2",
    "react": "19.1.0",
    "react-bootstrap": "2.10.9",
    "react-dom": "19.1.0",
    "react-hook-form": "7.56.3",
    "react-spinners": "0.17.0",
    "recharts": "2.15.3",
    "sass": "1.89.1",
    "serve": "14.2.4",
    "typescript": "5.8.3",
    "uglify-js": "3.19.3",
    "uuid": "11.1.0"
  },
  "devDependencies": {
    "@eslint/compat": "1.2.9",
    "@eslint/eslintrc": "3.3.1",
    "@eslint/js": "9.26.0",
    "@next/bundle-analyzer": "15.3.2",
    "@testing-library/dom": "10.4.0",
    "@testing-library/jest-dom": "6.6.3",
    "@testing-library/react": "16.3.0",
    "@testing-library/user-event": "14.6.1",
    "@types/jest": "29.5.14",
    "@types/node": "22.15.17",
    "@types/react": "19.1.3",
    "@types/react-dom": "19.1.3",
    "@types/uglify-js": "3",
    "@types/uuid": "10.0.0",
    "@typescript-eslint/parser": "8.32.0",
    "cross-env": "7.0.3",
    "eslint": "9.26.0",
    "eslint-config-next": "15.3.2",
    "eslint-config-prettier": "10.1.5",
    "eslint-plugin-import": "2.31.0",
    "eslint-plugin-jest-dom": "5.5.0",
    "eslint-plugin-redundant-undefined": "1.0.0",
    "eslint-plugin-sonarjs": "3.0.2",
    "eslint-plugin-testing-library": "7.1.1",
    "jest": "29.7.0",
    "jest-environment-jsdom": "29.7.0",
    "jest-mock": "29.7.0",
    "jest-websocket-mock": "2.5.0",
    "jscpd": "4.0.5",
    "knip": "5.55.1",
    "next-router-mock": "1.0.1",
    "npm-package-json-lint": "8.0.0",
    "openapi-typescript": "7.8",
    "prettier": "3.5.3",
    "ts-jest": "29.3.2",
    "typescript-eslint": "8.32.0"
  },
  "engines": {
    "node": "22.x",
    "yarn": "4.x"
  },
  "npmpackagejsonlint": {
    "rules": {
      "prefer-absolute-version-dependencies": "error",
      "prefer-alphabetical-dependencies": "error",
      "prefer-absolute-version-devDependencies": "error",
      "prefer-alphabetical-devDependencies": "error"
    }
  },
  "jscpd": {
    "exitCode": 1,
    "pattern": "**/*.{ts,tsx}",
    "ignore": [
      "**/*.d.ts",
      "**/coverage/**",
      "**/node_modules/**",
      "**/*.test.{ts,tsx}"
    ],
    "format": [
      "typescript",
      "tsx"
    ]
  },
  "prettier": {
    "parser": "typescript",
    "semi": false,
    "trailingComma": "none"
  },
  "packageManager": "yarn@4.9.1"
}
